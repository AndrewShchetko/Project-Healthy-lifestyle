concept_phospholipids<-sc_node_not_relation;;
concept_phospholipids => nrel_main_idtf: [фосфолипиды](*<- lang_ru;;*);;
concept_phospholipids => nrel_main_idtf: [phospholipids](*<- lang_en;;*);;



definition -> ...
(*
    -> rrel_key_sc_element: concept_phospholipids;;
    <= nrel_sc_text_translation:...
        (*
            -> [Фосфолипиды — сложные липиды, сложные эфиры многоатомных спиртов и высших жирных кислот. Содержат остаток фосфорной кислоты и соединённую с ней добавочную группу атомов различной химической природы.](*<- lang_ru;;*);;
	    -> [Phospholipids - complex lipids, esters of polyhydric alcohols and higher fatty acids. They contain a phosphoric acid residue and an additional group of atoms of various chemical nature connected to it.](*<- lang_ru;;*);;
        *);;
    => nrel_main_idtf: [Опр.(фосфолипиды)](*<- lang_ru;;*);;
    => nrel_main_idtf: [Def.(phospholipids)](*<- lang_ru;;*);;
   
*);;





